<h1>Pick your cuisine preferences:</h1>

<%= form_for @user_preference do |f| %>
  <% Recipe::YUMMLY_SEARCH_VALUES.each do |search_value| %>
    <div class='food-preference'>
      <%= check_box_tag "user_preference[cuisine_preference][]", search_value['name'] %>
      <%= f.label search_value['name'] %>
  <% end %>
  </div>
  <%= f.submit "Let's Plan" %>
<% end %>
